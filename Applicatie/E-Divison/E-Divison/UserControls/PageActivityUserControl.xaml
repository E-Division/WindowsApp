    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:E_Divison.UserControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="590"
    d:DesignWidth="400">

    <Grid Background="#FF30ACEC">
        <TextBlock x:Name="tbPageName" HorizontalAlignment="Left" Margin="155,46,0,0" TextWrapping="Wrap" Text="PageName" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
